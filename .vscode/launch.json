// A launch configuration that compiles the extension and then opens it inside a new window
{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Typescript",
			"port": 5859,
			"timeout": 20000,
			"restart": true,
			"sourceMaps": true,
			"outFiles": [
				"${workspaceRoot}/server/out/**/*.js"
			]
		}
	]
}